@using Chat.Web.ViewModels
@using Microsoft.AspNetCore.Mvc.TagHelpers
@model IEnumerable<Chat.Web.ViewModels.UserVm>

<div>
    @foreach (UserVm item in Model)
        {
            <p><a asp-controller="Chat" asp-action="Private" asp-route-with="@item.UserName" title="Отправить личное сообщение">@item.UserName</a></p>
        }
</div>

